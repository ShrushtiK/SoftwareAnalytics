    "def plot_activity(in_file, bin_size=5):\n",
    "    with open(in_file) as f:\n",
    "        repos = json.load(f)\n",
    "    # get the number of pull requests for each repo in a list\n",
    "    pull_requests_count=[]\n",
    "    for repo in repos:\n",
    "        pull_requests_count.append(repo['num_pull_requests'])\n",
    "    \n",
    "    # Plot Frequency Histogram on commits count\n",
    "    plt.hist(pull_requests_count, bins=range(0, max(pull_requests_count)+bin_size, bin_size)) # get bins of size 'bin_size'\n",
    "    plt.gca().set(title='Frequency Histogram', xlabel= 'Pull requests count', ylabel='Frequency')\n",
    "    plt.show()\n",
    "    \n",
    "plot_activity(\"data/4_pull_requests.json\", bin_size=100)"
