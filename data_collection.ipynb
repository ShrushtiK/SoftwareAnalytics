    "def get_repo_pull_requests(repo_name):\n",
    "    pull_requests = []\n",
    "    page = 1\n",
    "    per_page = 100\n",
    "    url = f'https://api.github.com/repos/{repo_name}/pulls'\n",
    "    headers = {\n",
    "        'Authorization': f'token {_TOKEN}',\n",
    "        'Accept': 'application/vnd.github.v3+json'\n",
    "    }\n",
    "    while True:\n",
    "        params = {\n",
    "            'state': 'closed',\n",
    "            'page': page,\n",
    "            'per_page': per_page\n",
    "        }\n",
    "        response = requests.get(url, headers=headers, params=params)\n",
    "\n",
    "        while response.status_code == 403 or response.status_code == 409:\n",
    "            print(\"Rate limited. Waiting 5 minutes...\")\n",
    "            time.sleep(300)\n",
    "            response = requests.get(url)\n",
    "            \n",
    "        if response.status_code == 200:\n",
    "            page_pull_requests = response.json()\n",
    "            print(f\"Repository {repo_name} on page {page}\")\n",
    "            if len(page_pull_requests) == 0:\n",
    "                break  # No more pull requests to retrieve\n",
    "            pull_requests.extend(page_pull_requests)\n",
    "            page += 1\n",
    "        else:\n",
    "            print(f\"Error: {response.status_code} {response.reason}\")\n",
    "            break\n",
    "    return pull_requests\n",
    "\n",
    "def get_all_pull_requests(in_file, out_file):\n",
    "    with open(in_file) as f:\n",
    "        repos = json.load(f)\n",
    "    all_pull_requests=[]\n",
    "    for repo in repos:\n",
    "        repo_name = repo[\"name\"]\n",
    "        print(repo_name)\n",
    "    \n",
    "        pull_requests = get_repo_pull_requests(repo_name)\n",
    "        pull_requests_count=len(pull_requests)\n",
    "\n",
    "        all_pull_requests.append({\n",
    "            \"full_name\": repo_name,\n",
    "            \"num_pull_requests\": pull_requests_count,\n",
    "            \"pull_requests\": pull_requests\n",
    "        })\n",
    "\n",
    "    with open(out_file, \"w\") as outfile:\n",
    "        json.dump(all_pull_requests, outfile)   \n",
    "\n",
    "get_all_pull_requests(\"data/3b_collaborators_and_users_filtered.json\", \"data/4_pull_requests.json\")"
   ]
